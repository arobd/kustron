apiVersion: extensions/v1beta1
kind: Ingress
metadata:
  name: ingress
  annotations:
    # Uncomment following annotation and give appropriate compute address that
    # you have created for your service in specific project e.g. dev.
    # Ref- https://cloud.google.com/kubernetes-engine/docs/tutorials/http-balancer#step_5_optional_configure_a_static_ip_address
    #
    # kubernetes.io/ingress.global-static-ip-name: [your-compute-address-here]
    #
    # Uncomment following annotation to make ingress use the google managed cert
    # generated by config/cert.yml
    # Ref- https://cloud.google.com/kubernetes-engine/docs/how-to/managed-certs
    #
    # networking.gke.io/managed-certificates: {{app.name}}-cert
